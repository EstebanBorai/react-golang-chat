{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "baseUrl": "."
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./src/**/*.sass"
  ]
}
